{
  "name": "ai-job-finder",
  "version": "1.0.0",
  "description": "AI-powered job aggregation and matching system",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "start": "node dist/index.js",
    "search": "tsx scripts/run-search.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:mutation": "stryker run",
    "test:mutation:open": "stryker run && open reports/mutation/mutation.html",
    "secrets:check": "secretlint \"**/*\"",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "prepare": "husky"
  },
  "dependencies": {
    "@langchain/core": "^0.3.0",
    "@langchain/langgraph": "^0.2.0",
    "@langchain/openai": "^0.3.0",
    "@prisma/client": "^6.19.1",
    "@sentry/node": "^10.32.1",
    "axios": "^1.7.0",
    "bull": "^4.16.0",
    "dotenv": "^16.4.0",
    "express": "^4.21.0",
    "express-rate-limit": "^8.2.1",
    "grammy": "^1.38.4",
    "ioredis": "^5.4.0",
    "langfuse": "^3.38.6",
    "mammoth": "^1.11.0",
    "node-cron": "^4.2.1",
    "p-limit": "^7.2.0",
    "pdf-parse": "^1.1.1",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@secretlint/secretlint-rule-preset-recommend": "^11.2.5",
    "@stryker-mutator/core": "^9.4.0",
    "@stryker-mutator/typescript-checker": "^9.4.0",
    "@stryker-mutator/vitest-runner": "^9.4.0",
    "@types/bull": "^4.10.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/pdf-parse": "^1.1.5",
    "husky": "^9.1.7",
    "prisma": "^6.0.0",
    "secretlint": "^11.2.5",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
