# Database
DATABASE_URL=postgresql://jobfinder:jobfinder@localhost:5433/jobfinder

# Redis (optional)
REDIS_URL=redis://localhost:6379

# OpenRouter
OPENROUTER_API_KEY=sk-or-...
OPENROUTER_MODEL=xiaomi/mimo-v2-flash:free
# Optional: Multiple API keys for higher throughput (comma-separated)
# OPENROUTER_API_KEYS=sk-or-key1,sk-or-key2,sk-or-key3
# OPENROUTER_KEY_RATE_LIMIT=10

# SerpAPI
SERPAPI_API_KEY=...

# JobSpy (optional)
# Comma-separated proxy URLs for high-throughput scraping
# JOBSPY_PROXIES=http://user:pass@proxy1.com:8080,http://user:pass@proxy2.com:8080
# JOBSPY_PARALLEL_ENABLED=false
# JOBSPY_PARALLEL_WORKERS=10

# Server
PORT=3001
